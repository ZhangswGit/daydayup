<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swapServer.mapper.RoleMapper">

    <select id="findAllRole" resultMap="com.swapServer.mapper.CommonMapper.RoleResultMap">
        select
        r.*
        from role r where r.is_delete  = 0;
    </select>

    <select id="findRoleById" resultMap="com.swapServer.mapper.CommonMapper.RoleResultMap">
        select * from role where id = #{id}  where r.is_delete  = 0;
    </select>

</mapper>
