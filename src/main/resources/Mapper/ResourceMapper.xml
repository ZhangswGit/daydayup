<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.connecttoweChat.mapper.UserMapper">

    <select id="findAllResourceByRoleId" resultMap="com.connecttoweChat.mapper.CommonMapper.ResourceResultMap">
        select
        r.*
        from resource r where 1=1
        <include refid="com.connecttoweChat.mapper.CommonMapper.condition"/>
    </select>
    
</mapper>
